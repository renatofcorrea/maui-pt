	</Concept>
	<Concept rdf:about="http://logic.ufpe.br/ci/tbci#c_1145">
		<prefLabel xml:lang="pt">metadados de preservação</prefLabel>
		<broader rdf:resource="http://logic.ufpe.br/ci/tbci#c_1143"/>
